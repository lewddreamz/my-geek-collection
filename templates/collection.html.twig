{% extends 'base.html.twig' %}
{% block body %}
<div class="">
    <p>{{collection.name}}</p>
    <div class="add-entry">
        <p><a href="{{ path('search') }}">Добавить новую запись</a></p>
    </div>
</div>
<div>
    {% for entry in collection.entries %}
        <p>Название: {{ entry.metadata.title }}, год выхода: {{ entry.createdAt|date('Y-m-d') }}</p>
    {% endfor %}
</div>
{% endblock %}
